{
    "name": "signify-nz/silverstripe-searchfilter-arraylist",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "description": "Provides an ArrayList subclass that supports SearchFilter filtering syntax.",
    "homepage": "https://github.com/signify-nz/silverstripe-searchfilter-arraylist",
    "keywords": [
      "silverstripe",
      "searchfilter",
      "arraylist"
    ],
    "authors": [
      {
        "name": "Signify Ltd",
        "homepage": "https://www.signify.co.nz/"
      }
    ],
    "support": {
      "issues": "https://github.com/signify-nz/silverstripe-searchfilter-arraylist/issues"
    },
    "require": {
      "php": ">=7.1",
      "silverstripe/vendor-plugin": "^1.0 || ^2.0",
      "silverstripe/framework": "^4.0 || ^5.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "silverstripe/framework": "^4.10",
      "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
      "psr-4": {
        "Signify\\SearchFilterArrayList\\": "src/",
        "Signify\\SearchFilterArrayList\\Tests\\": "tests/"
      }
    }
  }
